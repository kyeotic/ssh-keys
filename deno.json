{
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env --watch src/server.ts",
    "deploy": "deployctl deploy --allow-sys --prod"
  },
  "imports": {
    "@kyeotic/server": "jsr:@kyeotic/server@^1.4.14"
  },
  "deploy": {
    "project": "kyeotic-ssh-keys",
    "exclude": ["/.vscode"],
    "include": ["src/", "keys/", "./deno.json"],
    "entrypoint": "src/server.ts"
  },
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext", "deno.ns", "deno.unstable"]
  }
}
